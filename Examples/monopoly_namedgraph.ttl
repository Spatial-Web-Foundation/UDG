@prefix Class: <hstp:org:swf:hsml:hsml:Class:> .
@prefix ColorGroup: <hstp:org:swf:hsml:hsml:Domain:Game:Monopoly:ColorGroup> .
@prefix Domain: <hstp:org:swf:hsml:hsml:Domain:> .
@prefix Hyperspace: <hstp:org:swf:hsml:hsml:Hyperspace> .
@prefix Monopoly: <hstp:org:swf:hsml:hsml:Domain:Game:Monopoly:> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

Monopoly:Property:Shape a sh:NodeShape;
    rdfs:label "Go"@en;
    rdfs:comment "The starting point of the game."@en;
    sh:property [
        sh:path Monopoly:propertyName ;
        sh:name "Property Name"@en ;
        sh:description "Name of the property"@en ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:nodeKind sh:Literal ;
    ],
    [
        sh:path Monopoly:propertyValue ;
        sh:name "Property Value"@en ;
        sh:description "Value of the property in the game"@en ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:decimal ;
        sh:nodeKind sh:Literal ;
    ],
    [
        sh:path Monopoly:ColorGroup ;
        sh:name "ColorGroup"@en ;
        sh:description "Color of the Neighborhood"@en ;
        sh:minCount 0 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:nodeKind sh:Literal ;
    ]    
.

Monopoly:Property:ColorGroup a sh:NodeShape;
    rdfs:label "Monopoly Property Color Group"@en;
    rdfs:comment "Shape for the color group of Monopoly properties."@en;
    sh:property [
        sh:path Monopoly:colorGroup ;
        sh:name "Color Group"@en ;
        sh:description "Color group of the property"@en ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:nodeKind sh:IRI ;
        sh:class ColorGroup:ColorGroup ;
    ].

Monopoly:ColorGroup:Shape a sh:NodeShape;
    rdfs:label "Color Group"@en;
    rdfs:comment "Color group of the property."@en;
    sh:property [
        sh:path Monopoly:colorGroup ;
        sh:name "Color Group"@en ;
        sh:description "Name of the color group"@en ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:in (ColorGroup:Brown ColorGroup:LightBlue ColorGroup:Pink ColorGroup:Orange ColorGroup:Red ColorGroup:Yellow ColorGroup:Green ColorGroup:DarkBlue ColorGroup:None);
        sh:nodeKind sh:IRI ;
        sh:class: ColorGroup:ColorGroup ;
    ]
.

Monopoly:Game:Shape a sh:NodeShape;
    rdfs:label "Monopoly Game"@en;
    rdfs:comment "Shape for the Monopoly game."@en;
    sh:property [
        sh:path  Hyperspace:properties ;
        sh:name "Monopoly Property"@en ;
        sh:description "Properties in the Monopoly game"@en ;
        sh:minCount 1 ;
        sh:maxCount 40 ;
        sh:nodeKind sh:IRI ;
        sh:class Monopoly:Property ;
    ] ;
.


Monopoly:Piece:Shape a sh:NodeShape;
    rdfs:label "Monopoly Piece"@en;
    rdfs:comment "Pieces used in the game of Monopoly."@en;
    sh:property [
        sh:path Monopoly:pieceName ;
        sh:name "Piece Name"@en ;
        sh:description "Name of the Monopoly piece"@en ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
        sh:nodeKind sh:Literal ;
    ] ;
    sh:property [
        sh:path Monopoly:pieceValue ;
        sh:name "Piece Value"@en ;
        sh:description "Value of the Monopoly piece in the game"@en ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:decimal ;
        sh:nodeKind sh:Literal ;
    ].

ColorGroup:Brown a ColorGroup:ColorGroup ;
    rdfs:label "Brown"@en ;
    rdfs:comment "Brown color group in Monopoly."@en .

ColorGroup:LightBlue a ColorGroup:ColorGroup ;
    rdfs:label "Light Blue"@en ;
    rdfs:comment "Light Blue color group in Monopoly."@en .
ColorGroup:Pink a ColorGroup:ColorGroup ;
    rdfs:label "Pink"@en ;
    rdfs:comment "Pink color group in Monopoly."@en .
ColorGroup:Orange a ColorGroup:ColorGroup ;
    rdfs:label "Orange"@en ;
    rdfs:comment "Orange color group in Monopoly."@en .
ColorGroup:Red a ColorGroup:ColorGroup ;
    rdfs:label "Red"@en ;
    rdfs:comment "Red color group in Monopoly."@en .
ColorGroup:Yellow a ColorGroup:ColorGroup ;
    rdfs:label "Yellow"@en ;
    rdfs:comment "Yellow color group in Monopoly."@en .
ColorGroup:Green a ColorGroup:ColorGroup ;
    rdfs:label "Green"@en ;
    rdfs:comment "Green color group in Monopoly."@en .
ColorGroup:DarkBlue a ColorGroup:ColorGroup ;
    rdfs:label "Dark Blue"@en ;
    rdfs:comment "Dark Blue color group in Monopoly."@en .  
ColorGroup:None a ColorGroup:ColorGroup ;
    rdfs:label "None"@en ;
    rdfs:comment "No color group in Monopoly."@en .

Monopoly:Property rdfs:subClassOf Class:Domain .
Monopoly:ColorGroup rdfs:subClassOf Class:Concept .
Monopoly:Game  rdfs:subClassOf Class:Agenda .

